 */


#include <stdio.h>

int main(void)
{
	union Number {
		char *str;
		int x;
		float y;
	};

	union Number num1;
	num1.x = 10;
	printf("x = %d \n",num1.x);


	num1.y = 50.5;
	printf("y = %f \n",num1.y);


	num1.str = "IEEEEEEE";
	printf("str = %s \n", num1.str);

	printf("")
}
